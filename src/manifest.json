{
  "name" : "TVMuse Comment Blocker",
  "version" : "1.0",
  "description" : "Censors non-link comments on tvmuse.com episode pages.",
  "content_scripts": [ 
    {
      "js": [ "jquery.min.js", "contentscript.js" ],
      "matches": [ "http://www.tvmuse.com/tv-shows/*/*/*" ,"http://www.tvmuse.com/movies/*", 
      "http://www.anime/*/*/*", "http://www.tvmuse.com/documentaries/*"],
      "run_at": "document_end"
    } 
  ],
  "icons" : {
    "16" : "tv-16.png",
    "48" : "tv-48.png",
    "128" : "tv-128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "http://www.tvmuse.com/"
  ]
}